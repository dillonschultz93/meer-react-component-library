{
  "name": "meer-react-component-library",
  "version": "1.0.0",
  "description": "A React component based off of the Meer design system",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "generate": "node ./utilities/create-component.js",
    "build": "rollup -c",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dillonschultz93/meer-react-component-library.git"
  },
  "keywords": [
    "react",
    "component",
    "library"
  ],
  "private": false,
